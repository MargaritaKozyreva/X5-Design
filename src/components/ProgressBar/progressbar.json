{
  "space": "progressbar",
  "line": {
    "size": {
      "s": {
        "height": "4px"
      },
      "m": {
        "height": "8px"
      },
      "l": {
        "height": "12px"
      }
    },
    "back": {
      "background-color": "#FBFBFB",
      "border-radius": "16px"
    },
    "progress": {
      "base": {
        "border-radius": "16px",
        "transition": "all .2s"
      }
    }
  },
  "circle": {
    "size": {
      "s": {
        "stroke-width": "4px",
        "r": 32,
        "cx": 34,
        "cy": 34
      },
      "m": {
        "stroke-width": "8px",
        "r": 44,
        "cx": 48,
        "cy": 48
      },
      "l": {
        "stroke-width": "12px",
        "r": 64,
        "cx": 70,
        "cy": 70
      }
    },
    "box": {
      "size": {
        "s": {
          "width": "68px",
          "height": "68px"
        },
        "m": {
          "width": "96px",
          "height": "96px"
        },
        "l": {
          "width": "140px",
          "height": "140px"
        }
      }
    },
    "back": {
      "stroke-miterlimit": 10,
      "stroke": "#2B2D33",
      "fill": "none"
    },
    "progress": {
      "base": {
        "stroke-miterlimit": 10,
        "fill": "none",
        "transform": "rotate(-90deg)",
        "transform-origin": "50%"
      }
    },
    "content": {
      "size": {
        "s": {
          "width": "56px",
          "height": "56px"
        },
        "m": {
          "width": "72px",
          "height": "72px"
        },
        "l": {
          "width": "104px",
          "height": "104px"
        }
      }
    }
  }
}